
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    
    // WARNING: These rules are for development purposes only and are NOT secure
    // for a production environment. They allow any signed-in user to read and write
    // to the entire database.
    
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
